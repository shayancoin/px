{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "pnpm -w i",
  "buildCommand": "pnpm -w --filter @repo/design-engine build && pnpm -w --filter @repo/generation-engine build && pnpm --filter ./apps/shell build",
  "outputDirectory": ".next",
  "devCommand": "pnpm --filter ./apps/shell dev",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "NEXT_PUBLIC_DEMO_MODE": "1",
    "LLM_PROVIDER": "gemini"
  }
}



