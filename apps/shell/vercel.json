{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "pnpm -C apps/shell install --frozen-lockfile",
  "buildCommand": "sh -c 'pnpm -C packages/design-engine build && pnpm -C packages/generation-engine build && pnpm -C apps/shell build'",
  "outputDirectory": ".next",
  "devCommand": "pnpm --filter ./apps/shell dev",
  "env": {
    "NEXT_PUBLIC_DEMO_MODE": "1",
    "LLM_PROVIDER": "gemini"
  }
}



